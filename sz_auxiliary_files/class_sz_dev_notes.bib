	Issn = {0035-8711},
	Journal = {Monthly Notices of the Royal Astronomical Society},
	Month = {07},
	Number = {3},
	Pages = {3928-3941},
	Title = {{Joint analysis of the thermal Sunyaev--Zeldovich effect and 2MASS galaxies: probing gas physics in the local Universe and beyond}},
